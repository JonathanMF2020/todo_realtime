<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Todo_realtime" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary</item>
        <!-- Status bar color. -->
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:fitsSystemWindows">false</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Todo_realtime.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ExamenAndroid.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ExamenAndroid.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Button.White" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">@android:color/white</item>
        <item name="colorPrimary">@color/primary</item>
    </style>

    <style name="background_white" parent="Theme.MaterialComponents.Light">
        <item name="android:background">@color/white</item>
        <item name="iconGravity">textStart</item>
        <item name="iconTint">@color/primary</item>
        <item name="textAllCaps">false</item>
        <item name="android:textColor">@color/black</item>
    </style>


</resources>